#include<stdio.h>
#include<string.h>

int main() {
    int length;
    char str1[100], str2[100];

    printf("Enter the length of the string: ");
    scanf("%d", &length);

    printf("Enter the string: ");
    scanf("%s", str1);

    // copying str1 to str2
    strcpy(str2, str1);

    printf("Copied string is: %s", str2);
    return 0;
}
